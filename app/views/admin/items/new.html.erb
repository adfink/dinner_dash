<%= form_for([:admin, @item]) do |f| %>
  <%= f.label :title, "title" %>
  <%= f.text_field :title %><br>
  <%= f.label :description, "description" %>
  <%= f.text_field :description %><br>
  <%= f.label :price, "price" %>
  <%= f.text_field :price %><br>
  <%= f.label :photo_url, "photo" %>
  <%= f.text_field :photo_url %><br>
  <%= f.submit "add item" %>
<% end  %>
