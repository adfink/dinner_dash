</div>
<div class="col-lg-6">Sally

</div>
</div>
<% flash.each do |name, message| %>
  <%= content_tag :div, message, :id => "flash_#{name}" %>
<% end %>

<%= yield %>

<%= render partial: 'layouts/footer' %>
</body>
</html>
